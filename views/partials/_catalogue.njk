{% if entries.length > 0 %}
<table class="govuk-table">
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th scope="col" class="govuk-table__header">Name (SBB)</th>
      <th scope="col" class="govuk-table__header govuk-!-width-two-thirds">Description (ABB)</th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    {% for entry in entries %}
    <tr class="govuk-table__row">
      <td class="govuk-table__cell">{{ entry.name }}</td>
      <td class="govuk-table__cell">{{ entry.description }}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>
{% else %}
There are no entries in this section of the catalogue.
{% endif %}
